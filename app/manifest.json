{
  "name": "Text HyperDrive",
  "description": "More than just a text editor.",
  "version": "0.1",
  "icons": {
    "16": "icon/16x16.png",
    "96": "icon/96x96.png",
    "128": "icon/128x128.png",
    "256": "icon/256x256.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "23.0.1261",
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "permissions": [
    "http://127.0.0.1/",
    "https://127.0.0.1/",
    "http://localhost/",
    "https://localhost/",
    "clipboardRead",
    "clipboardWrite",
    "storage"
  ]
}
